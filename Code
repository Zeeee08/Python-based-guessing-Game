import random

zahl = random.randint(1, 100)

geraten = False
while not geraten:
    tipp = int(input("Rate eine Zahl zwischen 1 und 100: "))

    if tipp < zahl:
        print("Zu klein!")
    elif tipp > zahl:
        print("Zu gro√ü!")
    else:
        print("Richtig!")
        geraten = True

print("Spiel beendet.")
